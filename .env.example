# Development mode (skip GitHub OAuth, use mock user)
DEV_MODE=true

# GitHub OAuth
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Security - IMPORTANT: Change these in production!
JWT_SECRET=generate-a-strong-random-secret-here
WEBHOOK_SECRET=generate-another-strong-random-secret-here

# Database (Platform)
DATABASE_URL=postgresql://openflow:openflow@localhost:5432/openflow

# Domain
BASE_DOMAIN=openflow.localhost
API_PORT=3001
WEB_PORT=5173

# Docker
DOCKER_SOCKET=/var/run/docker.sock
